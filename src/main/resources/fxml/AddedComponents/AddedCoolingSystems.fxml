         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="14.0" layoutY="216.0" prefHeight="618.0" prefWidth="1118.0">
         <columns>
            <TableColumn prefWidth="75.0" text="Description" />
            <TableColumn prefWidth="75.0" text="Price" />
            <TableColumn prefWidth="75.0" text="Name" />
            <TableColumn prefWidth="75.0" text="Manufacturer" />
            <TableColumn prefWidth="75.0" text="Serial number" />
            <TableColumn prefWidth="153.0" text="Width (CM)" />
            <TableColumn prefWidth="95.0" text="Height (CM)" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
