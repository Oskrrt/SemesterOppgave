<AnchorPane fx:id="componentPane" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sample.controllers.adminUserController">
   <children>
      <Button fx:id="backToAddedComponents" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#viewSwapper" prefHeight="65.0" prefWidth="125.0" text="Back" />
